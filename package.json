{
  "name": "cbsoft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NEXT_PUBLIC_STATIC=true npm run generate-events && npm run generate-messages && next build --turbopack",
    "start": "next start",
    "postbuild": "next-sitemap",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "prettier:check": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "prepare": "husky",
    "generate-messages": "npx tsc src/scripts/generateMessages.ts --outDir dist && node dist/scripts/generateMessages.js",
    "generate-events": "tsc -p tsconfig.scripts.json && node dist/src/scripts/generateEvents.js",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@sinclair/typebox": "^0.34.41",
    "bootstrap": "^5.3.8",
    "next": "15.5.2",
    "next-intl": "^4.3.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^9.1.7",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "typescript": "^5.9.2"
  }
}
